{
  "name": "authgear-portal",
  "version": "0.1.0",
  "scripts": {
    "start": "snowpack dev",
    "build": "snowpack build",
    "typecheck": "tsc",
    "fmt": "prettier --list-different --write './src/**/*.{js,ts,jsx,tsx,css,scss,html}'",
    "eslint": "eslint './src/**/*.{js,ts,jsx,tsx}'",
    "stylelint": "stylelint './src/**/*.{css,scss}'",
    "gentype": "apollo client:codegen --config ./apollo.portal.config.js --target typescript --customScalarsPrefix GQL_ && apollo client:codegen --config ./apollo.adminapi.config.js --target typescript --customScalarsPrefix GQL_"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "7.10.4",
    "@babel/plugin-transform-typescript": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@csstools/postcss-sass": "4.0.0",
    "@oursky/eslint-plugin": "3.3.1",
    "@oursky/stylelint-oursky": "1.0.1",
    "@snowpack/plugin-babel": "2.1.0",
    "@snowpack/plugin-build-script": "2.0.6",
    "@snowpack/plugin-webpack": "2.0.6",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "apollo": "2.30.3",
    "autoprefixer": "9.8.6",
    "eslint": "7.7.0",
    "graphql": "15.3.0",
    "normalize.css": "8.0.1",
    "postcss-cli": "7.1.1",
    "postcss-modules": "3.2.0",
    "postcss-scss": "2.1.1",
    "prettier": "2.0.5",
    "sass": "1.26.10",
    "snowpack": "2.9.3",
    "stylelint": "13.6.1",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "@apollo/client": "3.1.4",
    "@authgear/web": "0.1.0",
    "@fluentui/react": "7.132.0",
    "@oursky/react-messageformat": "1.0.0",
    "core-js": "3.6.5",
    "history": "5.0.0",
    "react": "17.0.0-rc.0",
    "react-dom": "17.0.0-rc.0",
    "react-router-dom": "6.0.0-beta.0"
  }
}
