{
  "name": "authgear-portal",
  "version": "0.1.0",
  "scripts": {
    "test": "jest",
    "start": "parcel serve --port 1234 --no-cache ./src/index.html",
    "build": "PARCEL_BUNDLE_ANALYZER=1 parcel build --no-cache ./src/index.html",
    "clean": "rm -rf ./dist/ ./parcel-bundle-reports/ && mkdir ./dist/ && touch ./dist/.gitkeep",
    "typecheck": "tsc",
    "fmt": "prettier --list-different --write './src/**/*.{js,ts,jsx,tsx,css,scss,html}'",
    "eslint": "eslint './src/**/*.{js,ts,jsx,tsx}'",
    "stylelint": "stylelint './src/**/*.{css,scss}'",
    "gentype": "apollo client:codegen --config ./apollo.portal.config.js --target typescript --customScalarsPrefix GQL_ --globalTypesFile ./src/graphql/portal/__generated__/globalTypes.ts && apollo client:codegen --config ./apollo.adminapi.config.js --target typescript --customScalarsPrefix GQL_ --globalTypesFile ./src/graphql/adminapi/__generated__/globalTypes.ts"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/preset-typescript": "7.13.0",
    "@oursky/eslint-plugin": "4.1.0",
    "@oursky/stylelint-oursky": "1.0.1",
    "@parcel/transformer-sass": "2.0.0-beta.2",
    "@types/classnames": "2.2.11",
    "@types/deep-equal": "1.0.1",
    "@types/jest": "26.0.22",
    "@types/js-yaml": "4.0.0",
    "@types/luxon": "1.26.3",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/zxcvbn": "4.4.1",
    "apollo": "2.32.5",
    "eslint": "7.9.0",
    "graphql": "15.5.0",
    "jest": "26.6.3",
    "normalize.css": "8.0.1",
    "parcel": "2.0.0-beta.2",
    "postcss-modules": "3.2.2",
    "prettier": "2.2.1",
    "sass": "1.32.8",
    "stylelint": "13.8.0",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@apollo/client": "3.3.3",
    "@authgear/web": "0.5.0",
    "@fluentui/react": "7.157.2",
    "@monaco-editor/react": "3.7.2",
    "@oursky/react-messageformat": "1.0.0",
    "base64-js": "1.5.1",
    "classnames": "2.3.1",
    "deep-equal": "2.0.5",
    "google-libphonenumber": "3.2.19",
    "history": "5.0.0",
    "i18n-iso-countries": "6.6.0",
    "immer": "8.0.1",
    "js-yaml": "4.0.0",
    "luxon": "1.26.0",
    "postcss": "8.2.9",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.0.9",
    "react-router-dom": "6.0.0-beta.0",
    "zxcvbn": "4.4.2"
  }
}
