{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ES2015"],
    "noEmit": true,
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true  /* Disallow inconsistently-cased references to the same file. */,
    "resolveJsonModule": true
  }
}
