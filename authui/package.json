    "parcel": "2.3.1",
    "postcss": "8.4.6",
    "postcss-cli": "9.1.0",
    "prettier": "2.5.1",
    "tailwindcss": "2.2.19",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "axios": "0.26.1",
    "luxon": "2.3.0",
    "turbolinks": "5.2.0",
    "zxcvbn": "4.4.2"
